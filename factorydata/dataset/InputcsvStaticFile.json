{
	"name": "InputcsvStaticFile",
	"properties": {
		"linkedServiceName": {
			"referenceName": "InputBlob",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileNameStatic": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().FileNameStatic",
					"type": "Expression"
				},
				"container": "input"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "Id",
				"type": "String"
			},
			{
				"name": "FirstName",
				"type": "String"
			},
			{
				"name": "LastName",
				"type": "String"
			},
			{
				"name": "Address",
				"type": "String"
			},
			{
				"name": "Company",
				"type": "String"
			},
			{
				"name": "JobPosition",
				"type": "String"
			}
		]
	}
}